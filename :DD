sudo apt update
sudo apt install python3 python3-pip

sudo apt install net-tools arp-scan

sudo apt install python3-pyqt5

pip3 install requests
chmod +x your_script_name.py

sudo apt update
sudo apt upgrade -y

git clone https://github.com/darlene-git/fsociety-Control-.git

cd fsociety-Control- 
sudo apt install -y python3 python3-pip python3-pyqt5 git net-tools arp-scan

pip3 install -r requirements.txt

pip3 install PyQt5 requests netifaces


chmod +x name.py


chmod +x install.sh
./install.sh 

# Root yetkisi ile çalıştırın (ağ tarama için gerekli)
sudo python3 name.py 

curl -s https://raw.githubusercontent.com/darlene-git/fsociety-Control-/main/install.sh | bash
